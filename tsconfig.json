{
  "compilerOptions": {
    "target": "ES2020",
    "esModuleInterop": true,
    "noEmit": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": false,
  },
  "include": [
    "src/**/*.ts",
  ]
}
